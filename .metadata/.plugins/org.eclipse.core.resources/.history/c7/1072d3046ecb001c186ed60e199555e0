package chap04;

public class IfExample {

	public static void main(String[] args) {
		
		int score = 75;
		
		if(score >=90) {
			System.out.println("점수가 100-90입니다");
			System.out.println("등급은A입니다");
			
		}else if (score >=70) {
			System.out.println("점수가 80-90입니다");
			System.out.println("등급은b입니다");
		}

	}

}
